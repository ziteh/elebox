{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "elebox",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "app": {
        "all": false,
        "hide": false,
        "show": false
      },
      "protocol": {
        "asset": true,
        "assetScope": [
          "$LocalAppData/assets/*"
        ]
      },
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": false,
        "message": false,
        "open": true,
        "save": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev.elebox",
      "icon": [
        "icons/package-variant2.png",
        "icons/package-variant2.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Elebox",
        "width": 800,
        "height": 600
      }
    ]
  }
}
